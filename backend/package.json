{
  "name": "autofill-backend",
  "version": "1.0.0",
  "description": "自动填写工时后端服务",
  "main": "src/app.ts",
  "scripts": {
    "dev": "set NODE_ENV=dev&& ts-node ./src/app.ts",
    "test": "set NODE_ENV=prod&& node ./build/release/app.js",
    "build": "tsc",
    "start": "set NODE_ENV=prod&& node ./build/release/app.js",
    "forever-start": "set NODE_ENV=prod&& forever start --minUptime 1000 --spinSleepTime 3600000 ./build/release/app.js",
    "stop-all": "forever stopall&& forever cleanlogs",
    "run-list": "forever list"
  },
  "author": "jonexu",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.7",
    "express": "^4.17.2",
    "express-session": "^1.18.1",
    "file-stream-rotator": "^0.6.1",
    "lowdb": "^7.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^20.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}

